{
  "name": "react-eel-boilerplate",
  "version": "0.0.0",
  "description": "A simple boilerplate for creating simple applications using React and Eel.",
  "main": "dist/main.js",
  "repository": "git@github.com:hiukky/paley.git",
  "author": "Romullo",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist build *.spec",
    "dev": "concurrently \"yarn dev:react\" \"yarn dev:electron\" \"yarn dev:eel\"",
    "dev:electron": "webpack --config webpack/electron.config.js --mode development",
    "dev:react": "webpack-dev-server --config webpack/react.config.js --mode development",
    "dev:eel": "python3 app/main.py --develop",
    "build": "yarn clean && yarn build:react && yarn build:electron && yarn build:eel",
    "build:electron": "webpack --config webpack/electron.config.js --mode production",
    "build:react": "webpack --config webpack/react.config.js --mode production",
    "build:eel": "python3 -m eel app/main.py dist --onefile --noconsole --name react-eel-boilerplate",
    "start": "python3 app/main.py --production",
    "format": "prettier --write \"./**/*.{js,ts,tsx}\""
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "@minily/components": "^0.1.0-alpha.1",
    "@minily/tools": "^0.1.0-alpha.2",
    "copy-to-clipboard": "^3.3.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "remixicon": "^2.5.0",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.2",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.3.0",
    "css-loader": "^4.2.1",
    "electron": "^9.2.1",
    "eslint": "^7.7.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "style-loader": "^1.2.1",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
